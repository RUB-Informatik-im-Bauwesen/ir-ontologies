      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty loin:hasIdentifier ;
    ] ;
  vs:term_status "stable" ;
.
loin:BreakdownStructureType
