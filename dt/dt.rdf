    <dc:description xml:lang="en">The Data Template (DT) Ontology is based on concepts and principles for creating templates from ISO 23387 and the associated XML data schema, which is currently under development. </dc:description>
    <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dc:rights>Copyright 2024 by Chair of Computing in Engineering, Ruhr University Bochum and Chair of Construction Management, Bauhaus Universitaet Weimar</dc:rights>
    <owl:versionInfo>1.0</owl:versionInfo>
    <owl:versionInfo>Created with TopBraid Composer</owl:versionInfo>
    <dc:title xml:lang="en">Data Template (DT) Ontology</dc:title>
    <dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >https://github.com/RUB-Informatik-im-Bauwesen/ir-ontologies/tree/main/dt</dc:source>
  </owl:Ontology>
  <rdfs:Class rdf:ID="DataTemplate">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://w3id.org/loin#AlphanumericalInformation"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isDataTemplateFor"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdfs:Class rdf:ID="SetOfProperties"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasSetOfProperties"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://w3id.org/isoprops#Property"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <rdfs:Class rdf:ID="LibraryComponent"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Data template</rdfs:label>
    <rdfs:comment xml:lang="en">data structure used to describe the characteristics of construction objects</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="#SetOfProperties">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#SetOfProperties"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasSetOfProperties"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://w3id.org/isoprops#Property"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasSetOfProperties"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasProperty"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#LibraryComponent"/>
    </rdfs:subClassOf>
    <rdfs:comment>a set of properties that can be applied to a data template</rdfs:comment>
    <rdfs:label>Set of properties</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:ID="Unit">
    <owl:equivalentClass rdf:resource="http://qudt.org/schema/qudt/Unit"/>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#LibraryComponent"/>
    </rdfs:subClassOf>
    <rdfs:label>Unit</rdfs:label>
    <rdfs:comment>real scalar quantity, defined and adopted by convention, with which any other quantity of the same kind can be compared to express the ratio of the second quantity to the first one as a number</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:ID="ExternalDictionaryReference">
    <owl:equivalentClass rdf:resource="https://w3id.org/isoprops#ExternalDictionaryReference"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasExternalDictionaryProperty"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasExternalDictionary"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">reference to an external dictionary, which is a centralized repository of information about data such as meaning, relationships to other data, origin, usage and format</rdfs:comment>
    <rdfs:label>External dictionary</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="#LibraryComponent">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdfs:Class rdf:ID="ReferenceDocument"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasReferenceDocument"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#ExternalDictionaryReference"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasExternalDictionary"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Library component</rdfs:label>
    <rdfs:comment xml:lang="en">named and individually scheduled physical item and feature that might require management, such as inspection, maintenance, servicing or replacement, during the in-use phase</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:ID="Property">
    <owl:equivalentClass rdf:resource="https://w3id.org/isoprops#Property"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasPredefinedValues"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#LibraryComponent"/>
    <rdfs:label xml:lang="en-GB">Property</rdfs:label>
    <rdfs:comment xml:lang="en">inherent or acquired feature of an item</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:ID="PhysicalQuantity">
    <owl:equivalentClass rdf:resource="http://qudt.org/schema/qudt/QuantityKind"/>
    <rdfs:subClassOf rdf:resource="#LibraryComponent"/>
    <rdfs:comment xml:lang="en">the physical quantity of a library component</rdfs:comment>
    <rdfs:label>Physical quantity</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:ID="ConstructionObject">
    <rdfs:subClassOf rdf:resource="#LibraryComponent"/>
    <rdfs:label xml:lang="en">Construction object</rdfs:label>
    <rdfs:comment xml:lang="en">object of interest in the context of a construction process</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="#ReferenceDocument">
    <rdfs:comment xml:lang="en">publication that is consulted to find specific information, particularly in a technical or scientific domain</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="uri"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="ISBN"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="dateOfPublication"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="https://w3id.org/isoprops#DictionaryReferenceDocument"/>
    <rdfs:label xml:lang="en">Reference document</rdfs:label>
    <rdfs:subClassOf rdf:resource="#LibraryComponent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="publisher"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="author"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <owl:Class rdf:ID="PredefinedValueItem">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="hasIndex"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://schema.org/value"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://schema.org/PropertyValue"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">the physical quantity of a library component</rdfs:comment>
    <rdfs:label xml:lang="en-GB">Predefined value item</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="PredefinedValuesList">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasPredefinedValueItem"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://w3id.org/isoprops#hasUnit"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://w3id.org/isoprops#hasUnit"/>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">list of predefined values</rdfs:comment>
    <rdfs:label xml:lang="en-GB">Predefined value list</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#hasReferenceDocument">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="#ReferenceDocument"/>
    <rdfs:label>has reference document</rdfs:label>
    <rdfs:domain rdf:resource="#LibraryComponent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasSetOfProperties">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="#SetOfProperties"/>
    <rdfs:label>has set of properties</rdfs:label>
    <rdfs:domain rdf:resource="#SetOfProperties"/>
    <rdfs:domain rdf:resource="#DataTemplate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasProperty">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="https://w3id.org/isoprops#Property"/>
    <rdfs:label>has property</rdfs:label>
    <rdfs:domain rdf:resource="#DataTemplate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPredefinedValues">
    <owl:equivalentProperty rdf:resource="https://w3id.org/isoprops#hasPossibleValues"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="en-GB">Predefined Values</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasExternalDictionaryReference">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="#ExternalDictionaryReference"/>
    <rdfs:label>referenced external dictionary</rdfs:label>
    <rdfs:domain rdf:resource="#LibraryComponent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPredefinedValueItem">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label>Predefinded Value Item@en-GB}</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isDataTemplateFor">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="https://w3id.org/loin#SpecificationPerObjectType"/>
    <rdfs:label>is data template for</rdfs:label>
    <rdfs:domain rdf:resource="#DataTemplate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasExternalDictionary">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label>has external dictionary</rdfs:label>
    <rdfs:domain rdf:resource="#LibraryComponent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasExternalDictionaryProperty">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label>has external property reference</rdfs:label>
    <rdfs:domain rdf:resource="#ExternalDictionaryReference"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#uri">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label>uri</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#publisher">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>publisher</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#dateOfPublication">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label>date of publication</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#ISBN">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>ISBN</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#author">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>author</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hasIndex">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label>Index of the element in the enumeration</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

